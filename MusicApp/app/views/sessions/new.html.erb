<h1>Login!</h1>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>

  <label> Email:
  <input type="text" name="user[email]" placeholder="Enter Email">
  </label>

  <br />

  <label> Password:
  <input type="password" name="user[password]" placeholder="Password">
  </label>

  <br><br>

  <input type="submit">
</form>
