<h1>Cat: <%= @cat.name %></h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Birth Date</th>
      <th>Sex</th>
      <th>Color</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @cat.name %></td>
      <td><%= @cat.birth_date  %></td>
      <td><%= @cat.sex %></td>
      <td><%= @cat.color %></td>
      <td><%= @cat.description %></td>
    </tr>
  </tbody>
</table>
