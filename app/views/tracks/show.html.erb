<h1> <%= @track.title %> </h1>

<%= button_to "Edit Track", edit_track_url(@track), method: :get %>
<%= button_to "Delete Track", track_url(@track), method: :delete %>


<ul>
  <li> Album: <%= @track.album.title %> </li>
  <li> Track Type:  <%= @track.track_type %> </li>
  <li> Lyrics: <%= @track.lyrics  %> </li>
</ul>


<br> <br>

<%= link_to "Back to #{@track.album.title}", album_url(@track.album) %>
